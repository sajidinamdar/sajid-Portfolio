{
    "routes": [
        {
            "src": "^/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "^/(.*\\.(css|js|png|jpg|jpeg|gif|svg|pdf))$",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}